@model Vidly.ViewModels.RandomCustomerViewModel
@{
    ViewBag.Title = "Random Customer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@{
    var className = Model.Customers.
}

<h2 class-"customersNames">@Model.Customers.Name</h2>


@if (Model.Customers.Count == 0)
{
    <p>No one has rented this movie before.</p>
}
else
{
    <ul>
        @foreach (var customer in Model.Customers)
        {
            <li>
                @customer.Name
            </li>
        }
    </ul>
}

